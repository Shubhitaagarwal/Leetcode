class Solution {
public:
    string intToRoman(int num) {
        string str="";
        int n=0;
        n=num/1000;
        if(n!=0)
        {
            str=str.append(n,'M');
        }
        num=num%1000;
        n=num/100;
        if(n!=0 && n!=4 &&n!=9)
        {
            int a=n/5;
            int b=n%5;
            str=str.append(a,'D');
            str=str.append(b,'C');
        }
        else if(n==4)
        {
            str=str.append("CD");
        }
        else if(n==9)
        {
            str=str.append("CM");
        }
        num=num%100;
        n=num/10;
        if(n!=0 && n!=4 && n!=9)
        {
            int a=n/5;
            int b=n%5;
            str=str.append(a,'L');
            str=str.append(b,'X');
        }
        else if(n==4)
        {
            str=str.append("XL");
        }
        else if(n==9)
        {
            str=str.append("XC");
        }
        num=num%10;
        n=num;
        if(n!=0 && n!=4 && n!=9)
        {
            int a=n/5;
            int b=n%5;
            str=str.append(a,'V');
            str=str.append(b,'I');
        }
        else if(n==4)
        {
            str=str.append("IV");
        }
        else if(n==9)
        {
            str=str.append("IX");
        }
        return str;
    }
};
